{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && patch-package",
    "clean": "nuxt clean",
    "lint:js": "eslint --ext \".js,.cjs,.mjs,.ts,.cts,.mts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint": "run-s lint:js lint:style lint:types",
    "lint:ci": "npm-run-all postinstall --parallel lint:js lint:style lint:types",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lint:types": "nuxt typecheck",
    "lintfix": "prettier --write --list-different . && npm run lint -- --fix",
    "codegen": "openapi --input http://localhost:8001/openapi.json --output ./types/api"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@volar/vue-language-plugin-pug": "^1.6.5",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.38.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-pug": "^1.2.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-plugin-vue-pug": "^0.6.0",
    "openapi2aspida": "^0.21.0",
    "prettier": "^2.8.7"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@vuepic/vue-datepicker": "^7.4.0",
    "dayjs": "^1.11.9",
    "eslint-plugin-nuxt": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.6.5",
    "openapi-typescript-codegen": "^0.25.0",
    "patch-package": "^7.0.2",
    "postcss-html": "^1.5.0",
    "primevue": "^3.30.0",
    "pug": "^3.0.2",
    "sass": "^1.62.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.1.6",
    "vite-plugin-vuetify": "^1.0.2",
    "vuedraggable": "^4.1.0",
    "vuetify": "3.4.4",
    "vue-tsc": "^2.0.19"
  }
}
